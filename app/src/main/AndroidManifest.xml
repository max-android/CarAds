<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.carads">


    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />

    <application
        android:name=".di.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/key" />
        <activity
            android:name=".ui.primary.CarActivity"
            android:label="">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.detail.DetailActivity"
            android:label=""
            />
        <activity android:name=".ui.registration.LoginRegisterActivity" />
        <activity android:name=".ui.filter.FilterActivity" />
        <activity
            android:name=".ui.search.SearchableActivity"
            android:theme="@style/AppTheme" />
        <activity android:name=".ui.myads.MyAdsActivity" />
        <activity android:name=".ui.myads.AdvertisementActivity" />
        <activity android:name=".ui.setting.SettingsActivity" />
        <activity android:name=".ui.search.FavoritesActivity" />
        <activity android:name=".ui.map.MapActivity"></activity>
    </application>

</manifest> <!-- android:label="@string/app_name" -->
